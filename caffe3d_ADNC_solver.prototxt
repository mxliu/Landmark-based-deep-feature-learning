train_net: "caffe3d_ADNC_train.prototxt"
test_net: "caffe3d_ADNC_test.prototxt"
test_iter: 100
test_interval: 1000
base_lr: 0.001
momentum: 0.9
weight_decay: 0.0005
lr_policy: "step"
gamma: 0.1
stepsize: 15000
# Display every 100 iterations
display: 100
# The maximum number of iterations
max_iter: 100000
# snapshot intermediate results
snapshot: 1000
snapshot_prefix: "caffe3d_ADNC"
# solver mode: CPU or GPU
solver_mode: GPU
device_id: 0
